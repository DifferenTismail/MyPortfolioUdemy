@model List<Admin>
<a class="dropdown-toggle profile-pic" data-toggle="dropdown" href="#" aria-expanded="false"> <img src="@ViewBag.photo" alt="user-img" width="36" class="img-circle"><span>İsmail Baran</span></a>
<ul class="dropdown-menu dropdown-user">  
    @foreach(var item in Model){
    <li>
        <div class="user-box">
            <div class="u-img"><img src="@item.ImageUrl" alt="user"></div>
            <div class="u-text">
                <h4>@item.NameSurname</h4>
                <p class="text-muted">@item.Email</p><a href="#" class="btn btn-rounded btn-danger btn-sm">Mesaj Gönder</a>
            </div>
        </div>
    </li>
    }
    <div class="dropdown-divider"></div>
    <a class="dropdown-item" href="/Default/Index/"></i> Siteye Git</a>
    <a class="dropdown-item" href="/Message/Inbox/"><i class="ti-email"></i> Gelen Mesajlar</a>
    <div class="dropdown-divider"></div>
    <a class="dropdown-item" href="/Login/Logout/"><i class="fa fa-power-off"></i> Çıkış Yap</a>
</ul>